{
  "name": "apie/file-storage-plugin",
  "description": "Composer package of the apie library: file storage plugin",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\FileStoragePlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\FileStoragePlugin\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "apie/core": "dev-main",
    "pagerfanta/pagerfanta": "^2.4"
  },
  "require-dev": {
    "apie/object-access-normalizer": "dev-main",
    "apie/mock-objects": "dev-main",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}